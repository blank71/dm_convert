{%- import 'tf_macros.jinja' as macros %}
{%- set properties = resource.get('properties', {}) -%}

{%- extends "google_logging_sink_base.jinja" -%}
{%- block body -%}
{%- if 'parent' in properties %}
{%- set org_value = properties['parent'].split('/')[-1] %}
{%- else %}
{%- set org_value = properties['organization'] %}
{%- endif %}
#tfimport-terraform import google_logging_organization_sink.{{ macros.get_name(resource, properties) }} organizations/{{ org_value }}/sinks/{{ properties['name'] }}
resource "google_logging_organization_sink" "{{ macros.get_name(resource, properties) }}" {
  {{- macros.write_provider() }}
  {{- write_field_name('organization', 'org_id') }}
{%- endblock %}
