{%- import 'tf_macros.jinja' as macros %}
{%- set properties = resource.get('properties', {}) -%}
{%- set resource_name = resource.get('name') -%}
#tfimport-terraform import google_access_context_manager_service_perimeter_resource.{{ macros.get_name(resource, properties) }}  {{macros.import_project(properties['project'])}}/{{properties['region']}}/{{ resource_name }}
resource "google_access_context_manager_service_perimeter_resource" "{{ macros.get_name(resource, properties) }}" {
  {{- macros.write_provider() }}
  {{- macros.write_labels(properties) }}
  {{- macros.fields(properties, {
    'perimeterName': 'perimeter_name',
    'resource': 'resource',
    'useExplicitDryRunSpec': 'use_explicit_dry_run_spec',
    'perimeterType': 'perimeter_type',
    'description': 'description',
  }, ' ' * 2) }}
  {{- macros.format_dependencies(resource) | indent(2) }}
}
